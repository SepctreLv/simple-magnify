{
  "name": "simple-magify",
  "version": "0.0.1",
  "author": "SepctreLv",
  "license": "MIT",
  "homepage": "https://github.com/SepctreLv/simple-magnify",
  "scripts": {
    "start": "npm-run-all --sequential --parallel serve watch",
    "build": "npm-run-all clean img css:compile js:compile html:compile",
    "dist": "NODE_ENV=prod npm-run-all clean img css js html",
    "clean": "npm-run-all clean:*",
    "clean:css": "scripts/clean-css",
    "clean:js": "scripts/clean-js",
    "clean:html": "scripts/clean-html",
    "clean:img": "scripts/clean-img",
    "css": "npm-run-all css:beautify css:compile css:minify",
    "css:beautify": "scripts/beautify-css",
    "css:compile": "scripts/compile-css",
    "css:minify": "scripts/minify-css",
    "js": "npm-run-all js:beautify js:compile js:minify",
    "js:beautify": "scripts/beautify-js",
    "js:compile": "scripts/compile-js",
    "js:minify": "scripts/minify-js",
    "html": "npm-run-all html:compile html:minify",
    "html:compile": "scripts/compile-html",
    "html:minify": "scripts/usemin",
    "img": "npm-run-all img:minify",
    "img:minify": "scripts/minify-img",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "scripts/watch-js",
    "watch:css": "scripts/watch-css",
    "watch:html": "scripts/watch-html",
    "serve": "scripts/serve"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "autoprefixer": "^10.2.5",
    "browser-sync": "^2.26.14",
    "colors": "^1.4.0",
    "cssnano": "^5.0.1",
    "del": "^6.0.0",
    "front-matter": "^4.0.2",
    "fs-extra": "^9.1.0",
    "gaze": "^1.1.3",
    "glob-expand": "^0.2.1",
    "glob-parent": "^5.1.2",
    "handlebars-helpers": "^0.10.0",
    "handlebars-layouts": "^3.1.4",
    "handlebars-wax": "^6.1.0",
    "imagemin": "^7.0.1",
    "imagemin-giflossy": "^5.1.10",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "imagemin-webp": "^6.0.0",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-csso": "^5.0.1",
    "postcss-header": "^3.0.1",
    "postcss-reporter": "^7.0.2",
    "postcss-scss": "^3.0.5",
    "prettier": "^2.2.1",
    "rename": "^1.0.4",
    "rollup": "^2.45.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-es": "^3.3.9",
    "usemin": "^0.6.0"
  }
}